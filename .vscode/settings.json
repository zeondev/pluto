{
  "liveServer.settings.port": 5500,
  "liveServer.settings.ignoreFiles": [
    ".vscode/**",
    "**/*.scss",
    "**/*.sass",
    "**/*.ts",
    "**/*.md",
    "**/*.svg"
  ],
  "cSpell.words": [
    "Baro",
    "colspan",
    "FTGSF",
    "localforage",
    "lucide",
    "shrt",
    "titlebar",
    "Zeon"
],
  "cSpell.ignorePaths": [
    "package-lock.json",
    "node_modules",
    "vscode-extension",
    ".git/objects",
    ".vscode",
    ".vscode-insiders",
    "style.css"
  ]
}
